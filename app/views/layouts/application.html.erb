<!DOCTYPE html>
<html>
<head>
  <title>HnClone</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="navbar">
<% if !current_user %>
  <%= link_to "Sign In", sign_in_form_path %>
  <%= link_to "Sign Up", new_user_path %>
<% else %>
  <%= link_to "Sign Out", sign_out_path, method: :post %>
<% end %>
<%= yield %>

</body>
</html>
